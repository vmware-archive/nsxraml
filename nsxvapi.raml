          example: |
            <virtualWire>
              <name>ULS-Web-Tier-02 </name>
              <description>Universal Web Logical Switch</description>
              <tenantId>virtual wire tenant</tenantId>
              <controlPlaneMode>UNICAST_MODE</controlPlaneMode>
            </virtualWire>
          schema: logicalSwitchUpdate
