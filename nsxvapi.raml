  /verifyRootDn:
    displayName: verifyRootDn
    description: |
      Working with Root Distinguished Names
      ---------------
      Retrieve the list of individual root distinguished names under which each domain sub-tree synchronization is executed.
      
    post:
      displayName: verifyRootDnCreate
      description: | 
       Verify that the rootDNs in the rootDNList are independent to each other.
       Verify that the rootDNs in the rootDNList exist in Active Director server.
       
       **Method history:**
       
       Release | Modification
       --------|-------------
       6.4.0 | Method introduced.
       
      queryParameters:
         domainName:
           displayName: domainName
           description: Parent domain name this root distinguished name belongs to. It is a string type.
           required: true
         baseDN:
           displayName: baseDN
           description: Domain's base distinguished name. It is a string type.
           required: true
         rootDnlists: 
          displayName: rootDnList
          description: The list of individual root distinguished name. It is a list of string type.
          required: true 
         hostName:
           displayName: hostNameIPAddress
           description: Domain LDAP server host name or IP address. It is a list of string type.
           required: true
         port: 
           displayName: port
           description: Domain LDAP server port. It is an integer type.
           required: false
         username:
           displayName: username
           description: Account credential of user name to access LDAP server. It is a string type.
           required: true
         password:
           displayName: password
           description: Account credential of password to access ldap server. It is a string type.
           required: true
         protocol:
           displayName: protocol
           description: Domain LDAP server protocol used. It is a string type.
           required: false
      body:
        application/xml:
          example: |
            <RootDnVerify>
              <domainName>nimbustest.com</domainName>
              <baseDn>Example</baseDn>
              <rootDnItem>CN=nimbustest,DC=com</rootDnItem>
              <rootDnItem>CN=nimbustest,DC=com</rootDnItem>
              <hostName>10.142.72.70</hostName>
              <username>Administrator</username>
              <password>xxx</password>
            </RootDnVerify>
  /deleteDomainRootDN/{domainID}:
    displayName: DomainRootDN
    description: |
      Delete DomainRootDN
      -------
    uriParameters:
      domainID:
        required: true
        displayName: domainID
        description: Domain ID.
    delete:
      displayName: DomainRootDNDelete
      description: |
        Delete individual root distinguished name under which each domain sub-tree synchronization is not to be executed.
        
        **Method history:**
        
        Release | Modification
        --------|-------------
        6.4.0 | Method introduced.
  /updateLdapServer:
    displayName: ldapServerCreate
    description: |
      Create LDAP Server
      ------------
    post:
      displayName: ldapServerCreateCreate # dupfix ldapServerCreate
      description: Create LDAP server.
      body:
