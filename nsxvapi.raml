/2.0/nwfabric:
  displayName: nwfabric
  description: Network virtualization components
  /configure:
    displayName: nwfabricConfig
    description: Install components
    post:
      displayName: nwfabricConfigCreate
      description: |
         Install network virtualization components
      body:
        application/xml:
          schema: nwFabricConfig
          example: |
            Prepare a cluster (install VIBs) =>
            <nwFabricFeatureConfig>
              <resourceConfig>
                <resourceId>domain-c26</resourceId>
              </resourceConfig>
            </nwFabricFeatureConfig>

            VXLAN Context Configuration =>
            <nwFabricFeatureConfig>
              <featureId>com.vmware.vshield.vsm.vxlan</featureId>
              <resourceConfig>
                <resourceId>dvs-34</resourceId>
                <configSpec class="vdsContext">
                  <switch>
                    <objectId>dvs-34</objectId>
                  </switch>
                  <teaming>FAILOVER_ORDER</teaming>
                  <mtu>1600</mtu>
                </configSpec>
              </resourceConfig>
            </nwFabricFeatureConfig>

            VXLAN Cluster Prep =>
            <nwFabricFeatureConfig>
              <featureId>com.vmware.vshield.vsm.vxlan</featureId>
              <resourceConfig>
                <resourceId>domain-c26</resourceId>
                <configSpec class="clusterMappingSpec">
                  <switch>
                    <objectId>dvs-34</objectId>
                  </switch>
                  <ipPoolId>ipaddresspool-3</ipPoolId>
                  <vlanId>100</vlanId>
                  <vmknicCount>1</vmknicCount>
                </configSpec>
              </resourceConfig>
            </nwFabricFeatureConfig>
      queryParameters:
        action:
          displayName: action
          description: If used, set to "synchronize" to reset communication between NSX Manager and a host/cluster
    put:
      displayName: nwfabricConfigUpdate
      description: Upgrade network virtualization components
      body:
        application/xml:
          schema: nwFabricConfig
    delete:
      displayName: nwfabricConfigDelete
      description: Delete network virtualization components
      body:
        application/xml:
          schema: nwFabricConfig
