      ----
    uriParameters:
      scopeId:
        required: true
        displayName: scopeId
        description: |
          scopeId can be "globalroot-0", "universalroot-0" or datacenterID /
          portgroupID in upgrade use cases
    get:
      displayName: secGroupScopeRead
