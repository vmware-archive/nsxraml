          example: |
            <virtualWire>
              <objectId>universalwire-2</objectId>
              <objectTypeName>VirtualWire</objectTypeName>
              <vsmUuid>423A993F-BEE6-1285-58F1-54E48D508D90</vsmUuid>
              <nodeId>916287b3-761d-430b-8ab2-83878dfe3e7f</nodeId>
