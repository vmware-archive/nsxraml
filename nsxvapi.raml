  /resources/allocated:
    displayName: allocatedResources
    description: Working with allocated resources.
    get:
      displayName: allocatedResourcesRead
      description: |
        Retrieve information about allocated segment IDs or multicast
        addresses.
      queryParameters:
        type:
          displayName: type
          description: set to **segmentId** or **multicastAddress**
        pagesize:
          displayName: pagesize
          description: !include templates/documentation/reuse/pagesize-1024
          default: 256
        startindex:
          displayName: startindex
          description: !include templates/documentation/reuse/startindex
          default: 0
