              </type>
              <description></description>
              <name></name>
              <revision></revision>
              <objectTypeName></objectTypeName>
              <value></value>
              <inheritanceAllowed></inheritanceAllowed>
            </ipset>
          schema: ipsetCreate
  /{ipsetId}:
    displayName: ipset
    description: |
      Working with a Specific IP Set
      ----
    uriParameters:
      ipsetId:
        required: true
        displayName: ipsetId
