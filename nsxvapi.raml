                  <srcIp>172.32.1.5</srcIp>
                  <srcMac>00:50:56:83:7e:87</srcMac>
                  <dstMac>172.34.1.5</dstMac>
                  <lifMac>00:50:56:83:fa:6c</lifMac>
                </traceflowDto>
