                  <timezone>UTC</timezone>
                </timeSettings>
    put:
      displayName: systemTimeUpdate
      description: |
        Configure time or specify the NTP server to use for time
        synchronization.
      body:
        application/xml:
          example: |
            <timeSettings>
              <ntpServer>
                <string>192.168.110.1</string>
              </ntpServer>
              <datetime>04/19/2017 06:53:57</datetime>
              <timezone>UTC</timezone>
            </timeSettings>
