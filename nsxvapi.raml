                  </caCertificates>
                  <crlCertificates>
                    <crlCertificate></crlCertificate>
                  </crlCertificates>
                </global>
                <sites>
                  <site>
                    <enabled></enabled>
                    <name></name>
                    <description></description>
                    <localId></localId>
                    <localIp></localIp>
                    <peerId></peerId>
                    <peerIp></peerIp>
                    <encryptionAlgorithm></encryptionAlgorithm>
                    <authenticationMode>psk</authenticationMode>
                    <psk></psk>
                    <enablePfs></enablePfs>
                    <dhGroup></dhGroup>
                    <localSubnets>
