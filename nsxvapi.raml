                  <category></category>
                  <actionType></actionType>
                  <isActionEnforced></isActionEnforced>
                  <isActive></isActive>
                  <isEnabled></isEnabled>
                  <logged></logged>
                  <redirect></redirect>
                  <serviceProfile>
                    <objectId></objectId>
                  </serviceProfile>
                </action>
              </actionsByCategory>
            </securityPolicy>
          schema: securityPolicyCreate
